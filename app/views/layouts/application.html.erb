  <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
    <div class="navbar-nav">
      <a class="nav-item nav-link active" href="/static_pages/home">Home</a>
      <a class="nav-item nav-link" href="/static_pages/contact">Contact</a>
      <a class="nav-item nav-link" href="/static_pages/team">Team</a>
    </div>
  </div>
</nav>
    <%= yield %>
  </body>
</html>
